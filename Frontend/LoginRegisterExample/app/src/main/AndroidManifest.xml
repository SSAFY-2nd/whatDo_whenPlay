<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.loginregisterexample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LoginRegisterExample">
        <activity android:name=".MainActivity">
        </activity>
        <activity android:name=".SignupActivity"/>
        <activity android:name=".SubwayActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- 팝업 Activity -->
        <activity android:name=".PopupActivity" android:theme="@android:style/Theme.Holo.Dialog" />
    </application>
</manifest>