<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ssafy.play.dao.SubwayDAO">

	<select id="searchById" resultType="Subway">
		select *
		from subway where id = #{id}
	</select>
	
	<select id="searchByName" resultType="Subway">
		select *
		from subway where name = #{name}
	</select>
	
</mapper>