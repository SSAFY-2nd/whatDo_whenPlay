<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ssafy.play.dao.FoodCategorypostingDAO">
	
	<select id="searchBySubwayId" resultType="Integer">
		select category_id
		from foodcategoryposting where subway_id = #{subway_id}
		order by post desc
	</select>
	
	
	
</mapper>